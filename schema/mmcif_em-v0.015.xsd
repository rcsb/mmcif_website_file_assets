      &lt;mmcif_em:residual&gt;36&lt;/mmcif_em:residual&gt;
   &lt;/mmcif_em:em_electron_diffraction_shell&gt;
   &lt;mmcif_em:em_electron_diffraction_shell electron_diffraction_id=&quot;1&quot; id=&quot;2&quot;&gt;
