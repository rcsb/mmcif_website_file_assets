      <xsd:sequence>
         <xsd:element name="em_electron_diffraction_shell" minOccurs="0" maxOccurs="unbounded">
            <xsd:complexType>
               <xsd:all>
                  <xsd:element name="d_res_high" minOccurs="0" maxOccurs="1" nillable="true" type="xsd:integer">
                     <xsd:annotation>
                        <xsd:documentation xml:lang="en">
the highest resolution d-value for the resolution range.
5.0
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="d_res_low" minOccurs="0" maxOccurs="1" nillable="true" type="xsd:integer">
