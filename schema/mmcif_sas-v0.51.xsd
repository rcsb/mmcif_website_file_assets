   &lt;/mmcif_sas:sas_scan&gt;
   &lt;mmcif_sas:sas_scan id=&quot;4&quot;&gt;
      &lt;mmcif_sas:axis_id&gt;1&lt;/mmcif_sas:axis_id&gt;
