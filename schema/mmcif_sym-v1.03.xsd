      &lt;mmcif_sym:name_Schoenflies&gt;C2h.6&lt;/mmcif_sym:name_Schoenflies&gt;
   &lt;/mmcif_sym:space_group&gt;
&lt;/mmcif_sym:space_groupCategory&gt;


