                        </xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="xsd:decimal">
                           <xsd:minInclusive value="0.0" />
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
