   &lt;/mmCIF:geom_angle&gt;
&lt;/mmCIF:geom_angleCategory&gt;


         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="geom_angle" minOccurs="0" maxOccurs="unbounded">
