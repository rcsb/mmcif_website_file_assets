                        <xsd:simpleContent>
                           <xsd:extension base="xsd:decimal">
                              <xsd:attribute fixed="electrons" name="units" type="xsd:string" use="optional" />
                           </xsd:extension>
                        </xsd:simpleContent>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="B_calc_au" minOccurs="0" maxOccurs="1" nillable="true">
