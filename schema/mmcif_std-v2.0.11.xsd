      &lt;mmCIF:F_squared_sigma&gt;61.86&lt;/mmCIF:F_squared_sigma&gt;
      &lt;mmCIF:status&gt;o&lt;/mmCIF:status&gt;
   &lt;/mmCIF:refln&gt;
   &lt;mmCIF:refln index_h=&quot;5&quot; index_k=&quot;0&quot; index_l=&quot;0&quot;&gt;
