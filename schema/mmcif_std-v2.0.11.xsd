      &lt;mmCIF:subterm&gt;(-)-norcocaine&lt;/mmCIF:subterm&gt;
   &lt;/mmCIF:journal_index&gt;
   &lt;mmCIF:journal_index term=&quot;(-)-norcocaine&quot; type=&quot;S&quot;&gt;
      &lt;mmCIF:subterm xsi:nil=&quot;true&quot; /&gt;
   &lt;/mmCIF:journal_index&gt;
