      &lt;mmCIF:label_comp_id&gt;VAL&lt;/mmCIF:label_comp_id&gt;
      &lt;mmCIF:label_seq_id&gt;32&lt;/mmCIF:label_seq_id&gt;
      &lt;mmCIF:symmetry&gt;1_555&lt;/mmCIF:symmetry&gt;
   &lt;/mmCIF:struct_site_gen&gt;
