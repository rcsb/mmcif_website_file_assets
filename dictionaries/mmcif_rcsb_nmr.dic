    _reflns.limit_h_max                6
    _reflns.limit_k_min                0
    _reflns.limit_k_max                17
    _reflns.limit_l_min                0
    _reflns.limit_l_max                22
    _reflns.number_all                 1592
    _reflns.number_obs                 1408
    _reflns.observed_criterion         F_>_6.0_\s(F)
