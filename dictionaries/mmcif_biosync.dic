     'nanometres'   'nanometres  (meters * 10^( -9))'
     'angstroms'    'angstroms   (meters * 10^(-10))'
     'celsius'      'degrees (of temperature) Celsius'
     'kelvins'      'degrees (of temperature) Kelvin'
     'hours'        'hours'
     'minutes'      'minutes'
     'seconds'      'seconds'
     'microseconds' 'microseconds'
     'kilowatts'    'kilowatts'
     'milliamperes' 'milliamperes'
     'kilovolts'    'kilovolts'
