      &lt;PDBx:atom_2_comp_id&gt;1&lt;/PDBx:atom_2_comp_id&gt;
      &lt;PDBx:value_dist&gt;1.458&lt;/PDBx:value_dist&gt;
      &lt;PDBx:value_dist_esd&gt;0.019&lt;/PDBx:value_dist_esd&gt;
   &lt;/PDBx:chem_link_bond&gt;
