      &lt;PDBx:tilt_angle_min&gt;0&lt;/PDBx:tilt_angle_min&gt;
   &lt;/PDBx:em_imaging&gt;
&lt;/PDBx:em_imagingCategory&gt;


         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
