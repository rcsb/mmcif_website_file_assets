&lt;PDBx:citation_authorCategory&gt;
   &lt;PDBx:citation_author citation_id=&quot;primary&quot; name=&quot;Fitzgerald, P.M.D.&quot; ordinal=&quot;1&quot;&gt;&lt;/PDBx:citation_author&gt;
