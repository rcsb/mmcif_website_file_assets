   &lt;PDBx:phasing_MIR_der_shell d_res_high=&quot;3.8&quot; d_res_low=&quot;4.4&quot; der_id=&quot;KAu(CN)2&quot;&gt;
      &lt;PDBx:ha_ampl&gt;39&lt;/PDBx:ha_ampl&gt;
