      &lt;PDBx:asym_id&gt;A&lt;/PDBx:asym_id&gt;
      &lt;PDBx:auth_asym_id&gt;A&lt;/PDBx:auth_asym_id&gt;
      &lt;PDBx:auth_seq_id&gt;105&lt;/PDBx:auth_seq_id&gt;
      &lt;PDBx:comp_id&gt;TRP&lt;/PDBx:comp_id&gt;
