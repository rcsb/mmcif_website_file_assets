      &lt;PDBx:dev_ideal_target&gt;0.043&lt;/PDBx:dev_ideal_target&gt;
      &lt;PDBx:number&gt;39&lt;/PDBx:number&gt;
   &lt;/PDBx:refine_ls_restr&gt;
